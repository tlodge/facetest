{
    "manifest-version": 1,
    "name": "databox-browser-webcam-driver",
    "version": "0.5.0",
    "description": "A Databox driver to connect to a chrome webcam plugin",
    "author": "Tom Lodge <Thomas.Lodge@nottingham.ac.uk> ()",
    "license": "MIT",
    "databox-type": "driver",
    "tags": [
        "store",
        "blob",
        "db",
        "native",
        "json"
    ],
    "homepage": "https://github.com/tlodge/databox-browser-webcam-driver",
    "repository": {
        "type": "git",
        "url": "git+https://github.com/tlodge/databox-browser-webcam-driver.git"
    },
    "network-permissions": [],
    "external-whitelist": [],
    "hardware-permissions": [],
    "resource-requirements": {
        "store": "core-store"
    },
    "provides": [
        {
            "data-source-type": "browserWebcamImage",
            "description": "browser webcam image",
            "store-type": "kv",
            "schema": {}
        }
    ],
    "docker-image": "databox-browser-webcam-driver",
    "docker-registry": "tlodge",
    "docker-image-tag": "0.5.1"
}